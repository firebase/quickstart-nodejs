    "test": "mocha --timeout=10000"
  },
  "devDependencies": {
    "@firebase/testing": "^0.18.2",
    "@types/mocha": "7.0.2",
    "@testdeck/mocha": "0.1.0",
    "mocha": "7.1.1",
    "prettier": "1.19.1",
    "source-map-support": "0.5.16",
    "ts-node": "7.0.1",
    "typescript": "3.1.3"
  },
  "mocha": {
    "recursive": "test",
    "require": "source-map-support/register"
  }
}
